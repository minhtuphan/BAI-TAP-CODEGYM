    "import turtle\n",
    "import math\n",
    "r = float(input(\"nhập bán kính hình tròn: \"))\n",
    "def ve(r):\n",
    "    t = turtle.Turtle\n",
    "    t.circle(r)\n",
    "    turtle.done()\n",
    "def tinh_dien_tich(r):\n",
    "    return math.pi * r * r\n",
    "s = tinh_dien_tich(r)\n",
    "ve(r)\n",
    "print(f\"diện tích hình tròn bán kính {r:.0f} là: {s:.2f}\")"
