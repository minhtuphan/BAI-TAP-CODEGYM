     "text": [
      "nhập n: 100\n"
     ]
    }
   ],
   "source": [
    "import turtle\n",
    "t = turtle.Turtle()\n",
    "n = int(input(\"nhập n: \"))\n",
    "i = 0\n",
    "d = 5\n",
    "goc = 18\n",
    "t.penup()\n",
    "t.forward(d)\n",
    "t.right(goc)\n",
    "t.pendown()\n",
    "while i <= n:\n",
    "    t.forward(d)\n",
    "    t.left(goc)\n",
    "    d=d+0.5\n",
    "    i=i+1\n",
    "turtle.done()"
   ]
